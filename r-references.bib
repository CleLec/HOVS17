@article{sortheix2015,
  title = {Person-{{Group Value Congruence}} and {{Subjective Well-Being}} in {{Students}} from {{Argentina}}, {{Bulgaria}} and {{Finland}}: {{The Role}} of {{Interpersonal Relationships}}},
