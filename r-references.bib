@article{grosz2021,
  title = {The Longitudinal Interplay between Personal Values and Subjective Well-Being: {{A}} Registered Report},
  shorttitle = {The Longitudinal Interplay between Personal Values and Subjective Well-Being},
  author = {Grosz, Michael P and Schwartz, Shalom H. and Lechner, Clemens M},
  date = {2021-11},
  journaltitle = {European Journal of Personality},
  shortjournal = {Eur J Pers},
